{
  "name": "apollo-graphql-typescript",
  "version": "1.0.0",
  "description": "Template for serverless typed GraphQL app deployed on netlify",
  "main": "index.js",
  "scripts": {
    "build:lambda": "tsc -p src/lambda/tsconfig.json",
    "build:client": "webpack --mode production",
    "build": "graphql-codegen && run-p build:**",
    "start:dev-lambda": "tsc -p src/lambda/tsconfig.json -w",
    "start:dev-client": "webpack-dev-server --mode development --inline --hot",
    "start:dev-generate": "graphql-codegen -w",
    "start": "run-p start:**",
    "dev": "netlify dev",
    "check:lint": "eslint src/ --ext .tsx,.ts,.js,.jsx",
    "format": "prettier --write ."
  },
  "author": "brendondulam06@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@hookform/resolvers": "^1.3.2",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.19.1",
    "apollo-server-lambda": "^2.14.2",
    "clsx": "^1.1.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.11",
    "react": "^16.13.1",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.14.1",
    "react-router-dom": "^5.2.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@graphql-codegen/cli": "^1.14.0",
    "@graphql-codegen/typescript": "^1.14.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@types/mongodb": "^3.6.3",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "graphql-codegen-yup-schema": "^1.0.4",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:lint"
    }
  }
}
